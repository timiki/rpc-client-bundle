{
    "name": "timiki/rpc-client-bundle",
    "type": "symfony-bundle",
    "description": "Timiki RPC client bundle for symfony",
    "keywords": [
        "rpc",
        "json-rpc",
        "bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Netesin",
            "email": "nikita@netesin.ru"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/dependency-injection": "^5.0 || ^6.0",
        "symfony/http-kernel": "^5.0 || ^6.0",
        "symfony/config": "^5.0 || ^6.0",
        "timiki/rpc-client": "^4.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-symfony": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Timiki\\Bundle\\RpcClientBundle\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}