{
    "name": "timiki/rpc-client-bundle",
    "type": "symfony-bundle",
    "description": "Timiki RPC client bundle for symfony",
    "keywords": [
        "rpc",
        "json-rpc",
        "bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Netesin",
            "email": "nikita@netesin.ru"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "symfony/dependency-injection": "^4.3 || ^5.0",
        "symfony/http-kernel": "^4.3 || ^5.0",
        "symfony/config": "^4.3 || ^5.0",
        "timiki/rpc-client": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-symfony": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Timiki\\Bundle\\RpcClientBundle\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}