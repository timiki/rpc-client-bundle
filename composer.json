{
    "name": "timiki/rpc-client-bundle",
    "type": "symfony-bundle",
    "description": "Timiki RPC client bundle for symfony",
    "keywords": [
        "rpc",
        "json-rpc",
        "bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Netesin N",
            "email": "nikita@netesin.ru"
        }
    ],
    "require": {
        "symfony/dependency-injection": "^3.3 || ^4.0",
        "symfony/http-kernel": "^3.3 || ^4.0",
        "symfony/config": "^3.3 || ^4.0",
        "timiki/rpc-client": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpstan/phpstan": "^0.10.5",
        "phpstan/phpstan-symfony": "^0.10.2"
    },
    "autoload": {
        "psr-4": {
            "Timiki\\Bundle\\RpcClientBundle\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}